<Application x:Class="BiciTreningPlan.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:BiciTreningPlan"
             xmlns:vm="clr-namespace:BiciTreningPlan.ViewModel.TrainingPlan"
             xmlns:vw="clr-namespace:BiciTreningPlan.View.TrainingPlan"
             StartupUri="View/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <DataTemplate DataType="{x:Type vm:NewTrainingPlanViewModel}">
                <vw:NewTrainingPlan/>
            </DataTemplate>
            <DataTemplate DataType="{x:Type vm:RacesInSeasonViewModel}">
                <vw:RacesInSeason/>
            </DataTemplate>
            <DataTemplate DataType="{x:Type vm:RacesViewModel}">
                <vw:Races/>
            </DataTemplate>
                        
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
